{
  "name": "ca-system-builder",
  "description": "A Circulatory Autogen system builder application.",
  "author": {
    "name": "Hugh Sorby",
    "email": "h.sorby@auckland.ac.nz"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "app:dev": "vite --config vite.electron.config.js",
    "app:pack": "vite build --config vite.electron.config.js && electron-builder --dir",
    "app:build": "vite build --config vite.electron.config.js && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "nz.ac.auckland.circulatory-autogen.ca-system-builder",
    "productName": "CA System Builder",
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "src/electron/main.js"
    ],
    "directories": {
      "output": "dist-applications"
    }
  },
  "dependencies": {
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/core": "^1.47.0",
    "@vue-flow/minimap": "^1.5.4",
    "@vue-flow/node-resizer": "^1.5.0",
    "element-plus": "^2.11.5",
    "jszip": "^3.10.1",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue3-libcellml.js": "^0.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "vite": "^7.1.7",
    "vite-plugin-electron": "^0.29.0"
  }
}
